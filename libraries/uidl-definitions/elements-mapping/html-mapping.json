{
  "view": {
    "type": "div"
  },
  "group": {
    "type": "div"
  },
  "text": {
    "type": "span"
  },
  "heading1": {
    "type": "h1"
  },
  "image": {
    "type": "img",
    "attrs": {
      "src": "$attrs.url"
    }
  },
  "textinput": {
    "type": "input",
    "attrs": {
      "type": "text"
    }
  },
  "link": {
    "type": "a",
    "attrs": {
      "href": "$attrs.url",
      "target": "_blank"
    }
  },
  "button": {
    "type": "button"
  },
  "list": {
    "type": "ul",
    "childrenSource": "$attrs.items",
    "childStructure": {
      "type": "li",
      "children": ["$source"]
    }
  },
  "dropdown": {
    "type": "select",
    "childrenSource": "$attrs.options",
    "childStructure": {
      "type": "option",
      "children": ["$source"]
    }
  }
}
