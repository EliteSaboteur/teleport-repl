{
    "version": "1.1",
    "schema": "project",
    "root": "App",
    "name": "my-vue-project",
    "components": {

      "Layout": {
        "version": "v1",
        "name": "Layout",
        "propDefinitions": {
          "children": {
            "type": "children"
          }
        },
        "content": {
          "type": "Group",
          "name": "container",
          "children": [
            {
              "type": "View",
              "name": "Navbar",
              "children": [
                {
                  "type": "NavLink",
                  "name": "HomeLink",
                  "children": "Go Home",
                  "attrs": {
                    "transitionTo": "/"
                  }
                },
                {
                  "type": "NavLink",
                  "name": "AboutLink",
                  "children": "About Us",
                  "attrs": {
                    "transitionTo": "/about"
                  }
                }
              ]
            },
            "$props.children",
            {
              "type": "View",
              "name": "Footer",
              "children": "This is the footer"
            }
          ]
        }
      },
  
      "Home": {
        "version": "1",
        "name": "Home",
        "content": {
          "type": "Layout",
          "name": "HomeContent",
          "dependency": {
            "type": "local"
          },
          "style": {
            "textAlign": "center",
            "color": "yellow",
            "border": "1px solid #000"
          },
          "children": [{
            "type": "Button",
            "name": "Random Button",
            "children": "Click Me"
          },{
            "type": "Datepicker",
            "name": "DatePicker"
          },{
            "type": "Text",
            "name": "a-span",
            "children": "Welcome!"
          }]
        }
      },
  
      "About": {
        "version": "1",
        "name": "About",
        "content": {
          "type": "Layout",
          "name": "AboutContent",
          "dependency": {
            "type": "local"
          },
          "style": {
            "textAlign": "left",
            "color": "red",
            "border": "1px solid #000"
          },
          "children": [{
            "type": "View",
            "name": "about-container",
            "children": "This is the about page"
          }]
        }
      },
  
      "App": {
        "version": "1",
        "name": "App",
        "states": {  
          "home": {
            "content": {
              "name": "HomeInstance",
              "type": "Home",
              "dependency": { 
                "type": "local"
              }
            },
            "default": true
          },
  
          "about": {
            "content": {
              "name": "AboutInstance",
              "type": "About",
              "dependency": { 
                "type": "local"
              }
            }
          }
  
        }
      
      }
    }
  }