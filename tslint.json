{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "curly": false,
    "no-submodule-imports": false,
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "*.html",
      "**/*.html",
      "*.json",
      "**/*.json",
      "**/dist/**",
      "libraries/project-generators/vue-basic/project-template",
      "libraries/project-generators/react-basic/project-boilerplate"
    ]
  }
}